Question Vote Total: <%= vote_question.total_votes%>

<% if logged_in? && current_user != vote_question.user && current_user.has_voted_question?(vote_question) == false%>
  <%= button_to 'Upvote', upvote_question_path(vote_question), method: :post%>
  <%= button_to 'Downvote', downvote_question_path(vote_question), method: :post%>
<%end%>

